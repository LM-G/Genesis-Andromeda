{
    "bitwise": true,
    "curly": true,
    "forin": true,
    "freeze": true,
    "latedef": "nofunc",
    "noarg": true,
    "noempty": true,
    "nonbsp": true,
    "nonew": true,
    "undef": true,
    "unused": false,
    "strict": "global",
    "maxparams": 10,
    "maxdepth": 5,
    "maxstatements": 40,
    "maxcomplexity": 8,
    "eqnull": true,
    "loopfunc": true,
    "validthis": true,
    "-W030": true,
    /* disable "Expected an assignment or function call and instead saw an expression */
    "-W072": true,
    /* disable "This function has too many parameters" */
    "browser": true,
    "node": true,
    "globals": {
        "angular": false,
        "$": false
    }
}